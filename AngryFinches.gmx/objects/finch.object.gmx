        tmpid.phy_speed_y=phy_speed_y-2;
        //eo no4
        GFX_create(x+sprite_width/2,y-sprite_height/4,"b");
        
        tmpid=instance_create(x,y,finch_blueBirds);
        tmpid.phy_speed_x=phy_speed_x;
        tmpid.phy_speed_y=phy_speed_y;
        
        instance_destroy();
        //phy_speed_x*=0.8;
        SND_play("fbp");
    }
