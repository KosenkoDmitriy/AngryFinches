    with(other)do
    {
        if(!immo)
        {

                hp-=Gfinchdmg;
                if(Gfinchdmg&gt;1) Gfinchdmg*=0.5;
                else Gfinchdmg=0;
        
                
                ph=hp/hpmax;
                if(ph&gt;0.9) image_index=0;
                else if(ph&gt;0.7) image_index=1;
                else if(ph&gt;0.4) image_index=2;
                else if(ph&gt;0) image_index=3;
                else instance_destroy();
        }
    }
