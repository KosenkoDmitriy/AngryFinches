        tmpid.phy_speed_y=phy_speed_y+3;
        //eo no2
        GFX_create(x+sprite_width/2,y+sprite_height/4,"b");
        


        tmpid=instance_create(x+sprite_width/2,y-sprite_height/4,finch_blueBirds);
